        <researchPrerequisite>Craft_B1</researchPrerequisite>
    </RecipeDef>
    
    <RecipeDef ParentName="MintingRecipeBase">
        <defName>MakeSilverCoins</defName>
        <label>mint silver coins (х10)</label>
        <description>Mint silver coins. Produces 10.</description>
        <jobString>Minting silver coins.</jobString>
        <workAmount>1000</workAmount>
        <ingredients>
            <li>
                <filter>
                    <thingDefs>
                        <li>SilverCoinBlank</li>
                    </thingDefs>
                </filter>
                <count>10</count>
            </li>
        </ingredients>
        <fixedIngredientFilter>
            <thingDefs>
                <li>SilverBar</li>
            </thingDefs>
        </fixedIngredientFilter>
        <products>
            <SilverCoin>10</SilverCoin>
        </products>
        <skillRequirements>
            <Crafting>5</Crafting>
        </skillRequirements>
    </RecipeDef>

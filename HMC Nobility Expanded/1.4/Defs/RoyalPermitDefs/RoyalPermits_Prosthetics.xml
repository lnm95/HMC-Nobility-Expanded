    </RoyalTitlePermitDef>

    <RoyalTitlePermitDef ParentName="DropResourcesSpecific">
        <defName>SimpleProsthetic</defName>
        <label>Simple prosthetic</label>
        <description>Request a drop of specific simple prosthetic</description>
        <minTitle>Acolyte</minTitle>
        <cooldownDays>21</cooldownDays>
        <royalAid>
            <itemsToDrop>
                <Steel>1</Steel>
            </itemsToDrop>
            <favorCost>3</favorCost>
        </royalAid>
        <modExtensions>
            <li Class="NobilityExpanded.PermitExtensionList">
                <column>0</column>
                <thingsToChoose>
                    <ArtificialNose>1</ArtificialNose>
                    <SimpleProstheticHand>1</SimpleProstheticHand>
                    <SimpleProstheticFoot>1</SimpleProstheticFoot>
                    <SimpleProstheticLeg>1</SimpleProstheticLeg>
                    <SimpleProstheticArm>1</SimpleProstheticArm>
                    <SimpleProstheticHeart>1</SimpleProstheticHeart>
                    <ArtificialPelvis>1</ArtificialPelvis>
                </thingsToChoose>
            </li>
        </modExtensions>
    </RoyalTitlePermitDef>


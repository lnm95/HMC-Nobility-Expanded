<?xml version="1.0" encoding="utf-8" ?>
<Defs>
    <!-- Tools -->

    <RoyalTitlePermitDef ParentName="DropResourcesTitle">
        <defName>ProstheticsPermitTitle</defName>
        <label>Prosthetics</label>
        <description>Request a drop of various prosthetics.</description>
    </RoyalTitlePermitDef>

    <RoyalTitlePermitDef ParentName="DropResourcesSmallTitle">
        <defName>BasicProstheticsPermitSmallTitle</defName>
        <label>Basic Prosthetics</label>
        <description>Request a drop of different basic prosthetics.</description>
    </RoyalTitlePermitDef>

    <RoyalTitlePermitDef ParentName="DropResourcesSpecific">
        <defName>SimpleProsthetic</defName>
        <label>Simple prosthetic</label>
        <description>Request a drop of specific simple prosthetic</description>
        <minTitle>Acolyte</minTitle>
        <cooldownDays>21</cooldownDays>
        <royalAid>
            <itemsToDrop>
                <Steel>1</Steel>
            </itemsToDrop>
            <favorCost>4</favorCost>
        </royalAid>
    </RoyalTitlePermitDef>
    <NobilityExpanded.OrderedStuffDef ParentName="OrderedThingsSpecificBase">
        <defName>SimpleProstheticStuff</defName>
        <thingsToChoose>
            <li>ArtificialNose</li>
            <li>SimpleProstheticHand</li>
            <li>SimpleProstheticFoot</li>
            <li>SimpleProstheticLeg</li>
            <li>SimpleProstheticArm</li>
            <li>SimpleProstheticHeart</li>
            <li>ArtificialPelvis</li>
        </thingsToChoose>
    </NobilityExpanded.OrderedStuffDef>


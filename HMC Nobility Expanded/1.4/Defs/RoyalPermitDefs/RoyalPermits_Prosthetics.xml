<?xml version="1.0" encoding="utf-8" ?>
<Defs>
    <!-- Tools -->

    <RoyalTitlePermitDef ParentName="DropResourcesTitle">
        <defName>ProstheticsPermitTitle</defName>
        <label>Prosthetics</label>
        <description>Request a drop of various prosthetics.</description>
    </RoyalTitlePermitDef>

    <RoyalTitlePermitDef ParentName="DropResourcesSmallTitle">
        <defName>BasicProstheticsPermitSmallTitle</defName>
        <label>Basic Prosthetics</label>
        <description>Request a drop of different basic prosthetics.</description>
    </RoyalTitlePermitDef>

    <RoyalTitlePermitDef ParentName="DropResourcesSpecific">
        <defName>SimpleProsthetic</defName>
        <label>Simple prosthetic</label>
        <description>Request a drop of specific simple prosthetic</description>
        <minTitle>Acolyte</minTitle>
        <cooldownDays>21</cooldownDays>
        <royalAid>
            <itemsToDrop>
                <Steel>1</Steel>
            </itemsToDrop>
            <favorCost>3</favorCost>
        </royalAid>
        <modExtensions>
            <li Class="NobilityExpanded.PermitExtensionSpecificList">
                <thingsToChoose>
                    <ArtificialNose>1</ArtificialNose>
                    <SimpleProstheticHand>1</SimpleProstheticHand>
                    <SimpleProstheticFoot>1</SimpleProstheticFoot>
                    <SimpleProstheticLeg>1</SimpleProstheticLeg>
                    <SimpleProstheticArm>1</SimpleProstheticArm>
                    <SimpleProstheticHeart>1</SimpleProstheticHeart>
                    <ArtificialPelvis>1</ArtificialPelvis>
                </thingsToChoose>
            </li>
        </modExtensions>
    </RoyalTitlePermitDef>
    // удалить если найдено
    <NobilityExpanded.OrderedStuffDef ParentName="OrderedThingsSpecificBase">
        <defName>SimpleProstheticStuff</defName>
        <column>0</column>
        <thingsToChoose>
            <li>ArtificialNose</li>
            <li>SimpleProstheticHand</li>
            <li>SimpleProstheticFoot</li>
            <li>SimpleProstheticLeg</li>
            <li>SimpleProstheticArm</li>
            <li>SimpleProstheticHeart</li>
            <li>ArtificialPelvis</li>
        </thingsToChoose>
    </NobilityExpanded.OrderedStuffDef>

    <RoyalTitlePermitDef ParentName="DropResourcesSpecific">
        <defName>BionicsProsthetic</defName>
        <label>Bionic prosthetic</label>
        <description>Request a drop of specific bionic prosthetic</description>
        <minTitle>Legatus</minTitle>
        <cooldownDays>38</cooldownDays>
        <permitPointCost>3</permitPointCost>
        <royalAid>
            <itemsToDrop>
                <Steel>1</Steel>
            </itemsToDrop>
            <favorCost>7</favorCost>
        </royalAid>
        <modExtensions>
            <li Class="NobilityExpanded.PermitExtensionSpecificList">
                <column>1</column>
                <thingsToChoose>
                    <BionicHand>1</BionicHand>
                    <BionicFoot>1</BionicFoot>
                    <BionicEar>1</BionicEar>
                    <BionicJaw>1</BionicJaw>
                    <BionicSpine>1</BionicSpine>
                    <BionicEye>1</BionicEye>
                    <BionicTongue>1</BionicTongue>
                    <BionicLeg>1</BionicLeg>
                    <BionicNeck>1</BionicNeck>
                    <BionicLung>1</BionicLung>
                    <BionicStomach>1</BionicStomach>
                    <BionicLiver>1</BionicLiver>
                    <BionicKidney>1</BionicKidney>
                    <BionicHeart>1</BionicHeart>
                </thingsToChoose>
            </li>
        </modExtensions>
    </RoyalTitlePermitDef>
    <NobilityExpanded.OrderedStuffDef ParentName="OrderedThingsSpecificBase">
        <defName>BionicsProstheticStuff</defName>
        <column>1</column>
        <thingsToChoose>
            <li>BionicHand</li>
            <li>BionicFoot</li>
            <li>BionicEar</li>
            <li>BionicJaw</li>
            <li>BionicSpine</li>
            <li>BionicEye</li>
            <li>BionicTongue</li>
            <li>BionicLeg</li>
            <li>BionicNeck</li>
            <li>BionicLung</li>
            <li>BionicStomach</li>
            <li>BionicLiver</li>
            <li>BionicKidney</li>
            <li>BionicHeart</li>
        </thingsToChoose>
    </NobilityExpanded.OrderedStuffDef>


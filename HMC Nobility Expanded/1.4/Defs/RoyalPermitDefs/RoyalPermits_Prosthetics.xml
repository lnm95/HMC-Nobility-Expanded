<?xml version="1.0" encoding="utf-8" ?>
<Defs>
    <!-- Tools -->

    <RoyalTitlePermitDef ParentName="DropResourcesTitle">
        <defName>ProstheticsPermitTitle</defName>
        <label>Prosthetics</label>
        <description>Request a drop of various prosthetics.</description>
    </RoyalTitlePermitDef>

    <RoyalTitlePermitDef ParentName="DropResourcesSmallTitle">
        <defName>ProstheticsPermitSmallTitle</defName>
        <label>Artificial Bodyparts</label>
        <description>Request a drop of different prosthetics.</description>
    </RoyalTitlePermitDef>

    <RoyalTitlePermitDef ParentName="DropResourcesSmallTitle">
        <defName>CombatProstheticsPermitSmallTitle</defName>
        <label>Combat Implants and Bodyparts</label>
        <description>Request a drop of different combat prosthetics.</description>
    </RoyalTitlePermitDef>

    <RoyalTitlePermitDef ParentName="DropResourcesSmallTitle">
        <defName>ProductiveProstheticsPermitSmallTitle</defName>

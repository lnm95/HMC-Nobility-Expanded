    <RoyalTitlePermitDef ParentName="DropResourcesTitle">
        <defName>SeedsPermitTitle</defName>
        <label>Seeds</label>
        <description>Call for a drop of random seeds.</description>
    </RoyalTitlePermitDef>

    <RoyalTitlePermitDef ParentName="DropResourcesPlus">
        <defName>SeedsPotatoDrop</defName>
        <label>grains seeds drop</label>
        <description>Call for a drop of 75 random grains seeds.</description>
        <minTitle>Acolyte</minTitle>
        <cooldownDays>15</cooldownDays>
        <royalAid>
            <itemsToDrop>
                <Seed_Potato>75</Seed_Potato>
            </itemsToDrop>
            <favorCost>2</favorCost>
        </royalAid>
    </RoyalTitlePermitDef>
    <OrderedStuffDef ParentName="OrderedThingsBase">
        <defName>SeedsPotatoDropStuff</defName>
        <thingsToChoose>
            <li>Seed_Rice</li>
            <li>Seedwheat</li>
            <li>Seed_Corn</li>
            <li>Seedbean</li>
            <li MayRequire="arpomo6.hmc.biomes">SeedBuckwheat</li>
        </thingsToChoose>
    </OrderedStuffDef>


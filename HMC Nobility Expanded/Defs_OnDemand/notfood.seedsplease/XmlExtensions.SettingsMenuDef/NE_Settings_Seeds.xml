                                    <!-- Стоимость -->
                                    <li Class="XmlExtensions.DefDatabaseOperationReplace">
                                        <objPath>RoyalTitlePermitDef/[defName="SeedsGrainsDrop"]/permitPointCost</objPath>
                                        <value>
                                            <permitPointCost>{Grains_Cost}</permitPointCost>
                                        </value>
                                    </li>
                                    <li Class="XmlExtensions.DefDatabaseOperationReplace">
